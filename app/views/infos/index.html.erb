<h1>Liste des infos utilisateurs</h1>

<table>
  <tr>
	<th>Id</th>
    <th>Age</th>
    <th>Taille</th>
    <th>Sexe</th>
    <th></th>
    <th></th>
  </tr>

<% @infos.each do |info| %>
  <tr>
	<td><%= info.id %></td>
    <td><%= info.age %></td>
    <td><%= info.taille %></td>
    <td><%= info.sexe %></td>
    <td><%= link_to 'Show', info %></td>
    <td><%= link_to 'Modify', edit_info_path(info) %></td>
    <td><%= link_to 'Delete', info, :method => :delete, :confirm => 'Are you sure?', %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New calcul', new_info_path %>
